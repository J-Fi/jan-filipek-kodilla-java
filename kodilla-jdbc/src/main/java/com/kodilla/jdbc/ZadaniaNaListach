select SUMMARY, LISTNAME
from ISSUES I
join ISSUESLISTS IL ON I.ISSUESLIST_ID = IL.ID;

select SUMMARY, FIRSTNAME, LASTNAME
from ISSUES I
join USERS U ON I.USER_ID_ASSIGNEDTO = U.ID;

select FIRSTNAME, LASTNAME, count(*) as ISSUES_NUMBER
from ISSUES I
join USERS U ON I.USER_ID_ASSIGNEDTO = U.ID
group by U.ID;